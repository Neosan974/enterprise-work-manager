{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "nuxt",
    "nuxtjs",
    "primevue",
    "supabase",
    "tailwindcss",
    "valibot",
    "vexip",
    "vueuse"
  ],
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": true,
  "cSpell.language": "en,fr",
  "cSpell.languageSettings": [
    {
      "caseSensitive": false,
      "languageId": []
    }
  ]
}
